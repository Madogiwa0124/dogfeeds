{
  "name": "dogfeeds",
  "private": true,
  "scripts": {
    "build": "webpack --config './config/webpack/webpack.prod.js'",
    "build:clean": "rm -rf public/packs/* public/packs-test/*",
    "build:dev": "webpack --progress --config './config/webpack/webpack.dev.js'",
    "build:dev-watch": "webpack --watch --progress --config './config/webpack/webpack.dev.js'",
    "build:prod": "webpack --config './config/webpack/webpack.prod.js'",
    "build:test": "webpack --config './config/webpack/webpack.test.js'",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,vue,ts,css,scss}'",
    "lighthouse": "lhci autorun",
    "lint:js": "eslint app/javascript --ext .vue,.js,.ts",
    "lint:js-fix": "eslint app/javascript --ext .vue,.js,.ts --fix",
    "lint:css": "stylelint \"**/*.vue\" \"**/*.css\" \"**/*.scss\"",
    "type:check": "tsc -p . --noEmit && vtc --workspace . --srcDir app/javascript/components/ --onlyTypeScript true"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@rails/ujs": "^6.1.0",
    "@vue/composition-api": "^1.0.0-beta.22",
    "axios": "^0.21.1",
    "bulma": "^0.9.1",
    "bulma-extensions": "^6.2.1",
    "moment": "^2.29.1",
    "vue": "^2.6.12",
    "vue-infinite-loading": "^2.4.5",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.12.1",
    "@lhci/cli": "0.6.x",
    "@types/axios": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.4.0",
    "mini-css-extract-plugin": "^1.0.0",
    "prettier": "2.2.1",
    "rollbar-sourcemap-webpack-plugin": "^3.1.0",
    "sass": "^1.26.3",
    "sass-loader": "^10.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "vue-type-check": "^1.1.0",
    "webpack": "^4.42.1",
    "webpack-assets-manifest": "~3",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.7.3"
  }
}
