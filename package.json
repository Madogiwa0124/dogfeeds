{
  "name": "dogfeeds",
  "private": true,
  "scripts": {
    "build": "yarn build:prod",
    "build:clean": "rm -rf public/packs/* public/packs-test/*",
    "build:dev": "webpack --progress --config './config/webpack/webpack.dev.js'",
    "build:dev-watch": "webpack --watch --progress --config './config/webpack/webpack.dev.js'",
    "build:prod": "webpack --config './config/webpack/webpack.prod.js'",
    "build:test": "webpack --config './config/webpack/webpack.test.js'",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,vue,ts,css,scss}'",
    "lighthouse": "lhci autorun",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:type-check",
    "lint:js": "eslint app/javascript spec/javascript --ext .vue,.js,.ts",
    "lint:js-fix": "yarn lint:js --fix",
    "lint:css": "stylelint \"**/*.vue\" \"**/*.css\" \"**/*.scss\"",
    "lint:type-check": "tsc -p . --noEmit",
    "test": "jest --verbose",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^3.0.0-4",
    "@rails/ujs": "^6.1.4",
    "axios": "^0.21.1",
    "bulma": "^0.9.3",
    "bulma-extensions": "^6.2.1",
    "moment": "^2.29.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "rollbar": "^2.24.0",
    "vue": "^3.1.4",
    "vue-infinite-loading": "^3.0.0-alpha.0-0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@lhci/cli": "0.8.x",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@vue/compiler-sfc": "^3.1.4",
    "@vue/test-utils": "^2.0.0-rc.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.30.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "mini-css-extract-plugin": "^1.6.1",
    "prettier": "2.3.2",
    "rollbar-sourcemap-webpack-plugin": "^3.3.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^16.2.0",
    "webpack": "^5.44.0",
    "webpack-assets-manifest": "~5",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
