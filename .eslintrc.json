{
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "rules": {
    "semi": "error",
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "no-console": 0,
    "vue/require-prop-types": false,
    "vue/prop-name-casing": false,
    "vue/attribute-hyphenation": false
  }
}
