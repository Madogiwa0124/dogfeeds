<h1 class="title"><%= @board.id %>:<%= @board.title %></h1>
<p class="label">Feeds<p>
<table class="feed-collection table is-striped">
  <thead>
    <tr>
      <th>title</th>
      <th>endpoint</th>
    </tr>
  </thead>
  <tbody>
    <% @board.feeds.each do |feed| %>
      <tr>
        <td><%= feed.title %></td>
        <td><%= link_to feed.endpoint %></td>
      </tr>
    <% end %>
  </tbody>
</table>
